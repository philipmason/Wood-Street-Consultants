:@template"ñ/**
 * @class Ext.Button
 * Used to create the base structure of an Ext.Button
 */
@mixin extjs-structure-button {
  .x-btn {
    @include inline-block;
  
    cursor: pointer;
    cursor: hand;
    
    * {
      cursor: pointer;
      cursor: hand;
    }
    
    button {
      display: block;
    
      white-space: nowrap;
      
      margin: 0;
      padding: 0;
      
      border: 0;

      &:-moz-focus-inner {
        border: 0;
        padding: 0;
      }
    }
    
    &.x-item-disabled {
      @include opacity(.6);
      
      cursor: default;
      * { cursor: default; }
    }
        
    @include extjs-structure-button-generate('small',
      $button-small-font-size,
      $button-small-font-weight,
      $button-small-padding,
      $button-small-border-radius,
      $button-small-border-radius-vertical,
      $button-small-border-width,
      $button-small-inner-border-width,
      $button-small-border-style,
      $button-small-icon-size);

    @include extjs-structure-button-generate('medium',
      $button-medium-font-size,
      $button-medium-font-weight,
      $button-medium-padding,
      $button-medium-border-radius,
      $button-medium-border-radius-vertical,
      $button-medium-border-width,
      $button-medium-inner-border-width,
      $button-medium-border-style,
      $button-medium-icon-size);

    @include extjs-structure-button-generate('large',
      $button-large-font-size,
      $button-large-font-weight,
      $button-large-padding,
      $button-large-border-radius,
      $button-large-border-radius-vertical,
      $button-large-border-width,
      $button-large-inner-border-width,
      $button-large-border-style,
      $button-large-icon-size);
  }
  
  @if $include-no-border-radius {
    .ext-generator,
    .ext-no-border-radius {
      .x-btn {
        @include border-radius(0);
        
        @if $button-border-radius > 0 {
          border-width: 0;
          
          table,
          tbody {
            border-width: 0;
          }
        }

        table {
          td {
            &.x-btn-tl,
            &.x-btn-tr,
            &.x-btn-bl,
            &.x-btn-br {
              width: $button-border-radius;
              height: $button-border-radius;
            }

            &.x-btn-tc,
            &.x-btn-bc {
              height: $button-border-radius;
            }

            &.x-btn-ml,
            &.x-btn-mr {
              width: $button-border-radius;
            }

            @if $button-border-radius == 0 {
              &.x-btn-tl,
              &.x-btn-tc,
              &.x-btn-tr,
              &.x-btn-bl,
              &.x-btn-bc,
              &.x-btn-br,
              &.x-btn-ml,
              &.x-btn-mr {
                display: none;
              }
            }
          }
        }
      }
    }
  }
}

/**
 * Mixin used to create the base structure for a specific button scale (small, medium, large)
 * @param {String} label The name of the button scale (small)
 * @param {Number} font-size
 * @param {Number} font-weight
 * @param {Number} padding
 * @param {Number} border-radius
 * @param {Number} border-width
 * @param {Number} inner-border-width
 * @param {Number} icon-size
 */
@mixin extjs-structure-button-generate($label, $font-size, $font-weight, $padding, $border-radius, $border-radius-vertical, $border-width, $inner-border-width, $border-style, $icon-size) {
  .x-btn-#{$label},
  &.x-btn-#{$label} {
    border: $border-width $border-style;
    @include border-radius($border-radius);
    @include border-radius($border-radius, $border-radius-vertical);
    
    // all browsers
    button {
      font-size: $font-size;
      font-weight: $font-weight;
      
      background-repeat: no-repeat;
    }
    
    // modern browsers
    > button {
      border: $inner-border-width $border-style;
      @include border-radius($border-radius);
      @include border-radius($border-radius, $border-radius-vertical);
      
      // the minimum padding is always the border-radius size
      @if $padding < $border-radius {
        $padding: $border-radius;
      }
      
      padding: $padding; 
    }
    
    //icon'd buttons
    &.x-btn-icon {
      button {
        background-position: $padding $padding;
      }
      
      &.x-btn-#{$label} {
        button {
          width: $icon-size + ($padding * 2);
          height: $icon-size + ($padding * 2);
        }
      }
    }
    
    &.x-btn-text-icon {
      button {
        background-position: 0 0;
      }
      
      //left
      &.x-btn-icon-#{$label}-left {
        button {
          padding-left: $icon-size + 2px;
          
          @if $label == 'small' {
            background-position: 1px 3px;
          } @else {
            background-position: 0 0;
          }
        }
      }
      
      //right
      &.x-btn-icon-#{$label}-right {
        button {
          padding-right: $icon-size + 2px;
          
          @if $label == 'small' {
            background-position: 99% 3px;
          } @else {
            background-position: 99% 0;
          }
        }
      }
      
      //top
      &.x-btn-icon-#{$label}-top {
        button {
          padding-top: $icon-size + ($padding * 2);
          
          background-position: 50% $padding;
        }
      }
      
      //bottom
      &.x-btn-icon-#{$label}-bottom {
        button {
          padding-bottom: $icon-size + ($padding * 2);
          
          background-position: 50% 80%;
        }
      }
    }
    
    @if $include-no-border-radius {
      // legacy browsers
      table {
        button {
          @if $padding > $border-radius {
            // if the border is 0, we need to also remove the border width + inner border width from the padding
            @if $button-border-radius > 0 {
              $padding: $padding - $border-radius;
            }
            @else {
              $padding: ($padding - $border-radius) - ($border-width + $inner-border-width);
            }
          }
          @else {
            $padding: $padding - $border-radius;
          }
        
          padding: $padding;
        }
        
        &.x-btn-icon {
          button {
            background-position: 0 0;
            
            padding: 0;
          }

          &.x-btn-#{$label} {
            button {
              width: $icon-size;
              height: $icon-size;
            }
          }
        }
        
        &.x-btn-text-icon {
          button {
            padding-left: 0;
            padding-right: 0;
          }
          
          &.x-btn-icon-#{$label}-left,
          &.x-btn-icon-#{$label}-right {
            button {
              min-height: $icon-size;
              
            }
          }
          
          //left
          &.x-btn-icon-#{$label}-left {
            button {
              padding-left: $icon-size + 2px;

              @if $label == 'small' {
                background-position: 1px 1px;
              } @else {
                background-position: 0 0;
              }
            }
          }

          //right
          &.x-btn-icon-#{$label}-right {
            button {
              padding-right: $icon-size + 2px;

              @if $label == 'small' {
                background-position: 99% 1px;
              } @else {
                background-position: 99% 0;
              }
            }
          }
          
          //top
          &.x-btn-icon-#{$label}-top {
            button {
              padding-top: $icon-size + $padding;

              background-position: 50% 0;
            }
          }

          //bottom
          &.x-btn-icon-#{$label}-bottom {
            button {
              padding-bottom: $icon-size;

              background-position: 50% 100%;
            }
          }
        }
      }
    }
  }
